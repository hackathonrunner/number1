    border-radius: 4px;
    width: 100%;
    color: #4e4d4d;
    padding: 14px;
    font-size: 16px;
    line-height: 1.35;
    -webkit-appearance: none;
       -moz-appearance: none;
            appearance: none;
    outline: none;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    position: relative;
    z-index: 1;
    display: block;
    cursor: pointer; }
    .Select select:hover, .Select-button:hover {
      color: #4e4d4d; }
    .Select select:focus, .Select-button:focus {
      color: #4e4d4d;
      z-index: 3;
