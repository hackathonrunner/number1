.request-form {
  /**
     * Note(bryan): This spits out a lot of styles that are crucial to the fixed modal look and feel
     * and need to be shared between the request form, request flow, and react request form.
     *
     * Styles created include:
     * &__persistent-header {...}
     * &__close-modal {
     *     ...
     *     &__icon {
     *         ...
     *         &--branded {...}
     *     }
     * }
     * &__progress-container {...}
     * &__section-content {
     *     ...
     *     &__inner {...}
     * }
     * &__section-footer {
     *     ...
     *     &--branded {...}
     *     &__inner {...}
     *     &__skip-button,
     *     &__back-button {...}
     * }
     * &__dummy-section-footer {...}
     * &__contained-answers {...}
     * &__validation-error {...}
     */
  /**
     * This outputs:
     * &__explanation {
     *     ...
     *     &__container {...}
     *     &__logo {...}
     *     &__title {...}
     *     &__subtitle {...}
     * }
     */ }
