      .Hero.theme-large {
        padding: 80px 0 40px; } }
  .Hero.theme-small {
    min-height: initial;
    padding: 30px 0; }
  @media only screen and (min-width: 768px) {
    .Hero.theme-top-align {
      background-position: top center; } }
